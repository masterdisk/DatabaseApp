n        <div class=\"event-images columns is-multiline has-text-centered\">\r\n          <div v-for=\"image in event.images\" :key=\"image.id\" class=\"column is-one-third\">\r\n            <img :alt=\"`${event.name}`\" :src=\"`${image}`\"/>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\n// import EventService\r\nimport EventService from '@/services/EventService.js';\r\n\r\nexport default {\r\n  name: 'EventSingle',\r\n  data() {\r\n    // initialize the event object\r\n    return {\r\n      event: {}\r\n    }\r\n  },\r\n  created() {\r\n    this.getEventData();\r\n  },\r\n  methods: {\r\n    async getEventData() {\r\n      // Get the access token from the auth wrapper\r\n      const accessToken = await this.$auth.getTokenSilently()\r\n\r\n      // Use the eventService to call the getEventSingle method\r\n      EventService.getEventSingle(this.$route.params.id, accessToken)\r\n          .then(\r\n              (event => {\r\n                this.$set(this, \"event\", event);\r\n              }).bind(this)\r\n          );\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}